# OAuth Configuration
# Copy this file to .env and fill in your values

# Gitee OAuth
OAUTH_GITEE_CLIENT_ID=your-gitee-client-id
OAUTH_GITEE_CLIENT_SECRET=your-gitee-client-secret
OAUTH_GITEE_REDIRECT_URI=http://localhost:5173/oauth/callback/gitee

# GitHub OAuth
OAUTH_GITHUB_CLIENT_ID=your-github-client-id
OAUTH_GITHUB_CLIENT_SECRET=your-github-client-secret
OAUTH_GITHUB_REDIRECT_URI=http://localhost:5173/oauth/callback/github

# JWT Configuration (optional, has defaults)
# JWT_SECRET=your-jwt-secret-key
# JWT_EXPIRATION=86400000
